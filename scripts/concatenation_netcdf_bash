# Changement de la dimension d'enregistrement (record) pour concaténer après selon le temps.
for file in `ls *`; do ncks --mk_rec_dmn time ${file} $(basename $file .nc)_record_dim_is_time.nc; done

# Concaténation
## GLP
### SEXTANT
ncrcat interpolated_complete_sextant_ghrsst.rds_0[1234567]of07_record_dim_is_time.nc interpolation_complete_glp_sextant_sst.nc
ncrcat interpolated_complete_sextant_chlorophyll_a.rds_0[12345]of05_record_dim_is_time.nc interpolation_complete_glp_sextant_chla.nc
ncrcat interpolated_complete_sextant_suspended_matters.rds_0[12345]of05_record_dim_is_time.nc interpolation_complete_glp_sextant_ssm.nc
ncrcat interpolated_complete_sextant_turbidity.rds_0[12345]of05_record_dim_is_time.nc interpolation_complete_glp_sextant_tur.nc

### COPERNICUS
ncrcat interpolated_complete_copernicus_vhm0.rds_0[123456789101112131415161718]of18_record_dim_is_time.nc interpolation_complete_glp_copernicus_hm0.nc
ncrcat interpolated_complete_cop_vhm0_ww.rds_0[123456789101112131415161718]of18_record_dim_is_time.nc interpolation_complete_glp_copernicus_ww.nc
ncrcat interpolated_complete_cop_vhm0_sw1.rds_0[123456789101112131415161718]of18_record_dim_is_time.nc interpolation_complete_glp_copernicus_sw1.nc
ncrcat interpolated_complete_cop_bottomt.rds_0[1234567891011]of11_record_dim_is_time.nc interpolation_complete_glp_copernicus_sbt.nc
ncrcat interpolated_complete_cop_so-9_0[1234567891011]of11_record_dim_is_time.nc interpolation_complete_glp_copernicus_so9.nc
ncrcat interpolated_complete_cop_so-25_0[1234567891011]of11_record_dim_is_time.nc interpolation_complete_glp_copernicus_so25.nc
ncrcat interpolated_complete_cop_so-77_0[1234567891011]of11_record_dim_is_time.nc interpolation_complete_glp_copernicus_so77.nc
ncrcat interpolated_complete_cop_so-130_0[1234567891011]of11_record_dim_is_time.nc interpolation_complete_glp_copernicus_so130.nc

